{
  "resources": [
    {
      "path": "data.csv",
      "name": "data",
      "profile": "tabular-data-resource",
      "scheme": "file",
      "format": "csv",
      "hashing": "md5",
      "encoding": "utf-8",
      "schema": {
        "fields": [
          {
            "name": "Year",
            "type": "integer",
            "title": "Année"
          },
          {
            "name": "Sex",
            "type": "string",
            "constraints": {
              "enum": [
                "Both sexes",
                "Female",
                "Male",
                ""
              ]
            },
            "title": "Sex"
          },
          {
            "name": "Age Group",
            "type": "string",
            "constraints": {
              "enum": [
                "15-19",
                "20-24",
                "25+",
                "25-44",
                "45+",
                "All age groups",
                ""
              ]
            },
            "title": "Age Group"
          },
          {
            "name": "Tobacco Product",
            "type": "string",
            "constraints": {
              "enum": [
                "Any tobacco product",
                "Chewing tobacco",
                "Cigarettes",
                "Cigars",
                "E-cigarettes",
                "Little cigars or cigarillos",
                "Pipe",
                "Smokeless tobacco",
                "Vaping",
                "Water pipe with tobacco",
                ""
              ]
            },
            "title": "Tobacco Product"
          },
          {
            "name": "Value",
            "type": "number",
            "title": "Value"
          }
        ]
      }
    }
  ],
  "profile": "data-package",
  "name": "3-a-1",
  "title": "Prévalence de la consommation actuelle de tabac chez les personnes de 15 ans ou plus (taux comparatifs par âge)"
}